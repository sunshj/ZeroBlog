{
  "name": "nuxt-app",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --host",
    "dev:clean": "rimraf .nuxt && nuxt dev --host",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@giscus/vue": "3.0.0",
    "@nuxt/content": "2.13.4",
    "@nuxt/icon": "1.10.2",
    "@nuxt/image": "1.8.1",
    "@nuxtjs/color-mode": "3.5.2",
    "@sunshj/mdc": "0.3.3",
    "@unocss/nuxt": "65.5.0",
    "@vueuse/core": "11.2.0",
    "@vueuse/integrations": "11.2.0",
    "@vueuse/nuxt": "11.2.0",
    "feed": "4.2.2",
    "fuse.js": "7.0.0",
    "monaco-editor": "0.52.2",
    "nuxt": "3.15.4",
    "nuxt-auth-utils": "0.5.17",
    "nuxt-monaco-editor": "1.3.1",
    "nuxt-og-image": "4.2.0",
    "octokit": "4.1.2",
    "remark-reading-time": "2.0.1",
    "sitemap": "8.0.0",
    "undio": "0.2.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-use-fixed-header": "2.0.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "1.2.5",
    "@iconify-json/fa6-solid": "1.2.3",
    "@iconify-json/logos": "1.2.4",
    "@sunshj/eslint-config": "2.2.1",
    "@sunshj/prettier-config": "1.0.2",
    "@unocss/eslint-plugin": "65.5.0",
    "eslint": "9.15.0",
    "lint-staged": "15",
    "prettier": "3.3.3",
    "simple-git-hooks": "2.11.1",
    "unocss": "65.5.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,json,md,yaml}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@sunshj/prettier-config",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
